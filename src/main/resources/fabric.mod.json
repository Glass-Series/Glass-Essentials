{
  "schemaVersion": 1,
  "id": "glassessentials",
  "version": "${version}",

  "name": "Glass Essentials",
  "description": "A mod that aims to add a bunch of \"essential\" admin and player commands, along with a robust permissions system.",
  "authors": [
    "calmilamsy",
    "williambl"
  ],
  "contact": {
    "homepage": "https://glass-launcher.net/",
    "sources": "https://github.com/Glass-Series/Glass-Essentials"
  },

  "license": "MIT",
  "icon": "assets/glassbrigadier/icon.png",

  "environment": "*",
  "entrypoints": {
    "stationapi:event_bus": [
      "net.glasslauncher.glassbrigadier.impl.GlassBrigadier"
    ],
    "stationapi:event_bus_client": [
    ],
    "stationapi:event_bus_server": [
    ]
  },
  "mixins": [
    "glassbrigadier.mixins.json"
  ],

  "depends": {
    "minecraft": "1.0.0-beta.7.3"
  },

  "custom": {
    "loom:injected_interfaces": {
    }
